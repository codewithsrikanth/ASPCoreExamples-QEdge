@using MVCWebApp1.Models
@model List<Student>

<table>
	<tr>
		<th>Student ID</th>
		<th>Student Name</th>
		<th>Student Branch</th>
		<th>Student Section</th>
		<th>Student Gender</th>
	</tr>
	@{
		foreach(var item in Model)
		{
			<tr>
				<td>@item.StudentId</td>
				<td>@item.Name</td>
				<td>@item.Branch</td>
				<td>@item.Section</td>
				<td>@item.Gender</td>
			</tr>
		}
	}
</table>