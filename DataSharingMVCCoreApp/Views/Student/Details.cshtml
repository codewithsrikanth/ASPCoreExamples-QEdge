
@model DataSharingMVCCoreApp.Models.StudentDetailViewModel

<html>
	<head>
		<title>@Model?.Title</title>
	</head>
	<body>
	<h1>@Model?.Header</h1>
	<hr />
	<table>
		<tr>
			<th>StudentID</th>
			<th>Name</th>
			<th>State</th>
			<th>City</th>
		</tr>
		<tr>
			<td>@Model?.Student?.StudentId</td>
			<td>@Model?.Student?.Name</td>
			<td>@Model?.Address?.State</td>
			<td>@Model?.Address?.City</td>
		</tr>
	</table>
	</body>
</html>